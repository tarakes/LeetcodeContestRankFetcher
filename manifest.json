{
    "name": "Leetcode rank fetch",
    "version": "1.0",
    "description": "Add friend to monitor Leetcode contest rank",
    "manifest_version": 3,
    "author": "Tarakeswar Mondal",
    "action":{
        "default_popup": "index.html"
    },
    "icons": {"512": "leetcode_logo.png" },
    "content_scripts": [
        {
            "matches": ["https://leetcode.com/*"],
            "js": ["inject.js"]
        }
    ]

}